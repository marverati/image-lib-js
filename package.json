{
  "name": "image-lib-js",
  "version": "0.0.1",
  "description": "A TypeScript based library for pixel based image editing",
  "main": "index.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "parcel src/index.html",
    "build": "parcel build src/index.html",
    "build:editor": "npm run process-examples && parcel build src/editor/editor.html --out-dir dist/editor --public-url ./",
    "demo": "parcel src/demo/demo.html",
    "editor": "parcel src/editor/editor.html",
    "test": "jest",
    "dist": "npm run build:editor",
    "process-examples": "node scripts/process-examples.js",
    "dev:editor": "npm run process-examples && parcel src/editor/editor.html"
  },
  "author": "marverati",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.15",
    "jest": "^29.7.0",
    "parcel-bundler": "^1.12.5",
    "ts-jest": "^29.2.3",
    "typescript": "^4.9.3",
    "ts-node": "^10.9.2"
  },
  "dependencies": {
    "canvas": "^2.11.2"
  }
}
